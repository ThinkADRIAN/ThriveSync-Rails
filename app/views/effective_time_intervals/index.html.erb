<h1>Listing effective_time_intervals</h1>

<table>
  <thead>
    <tr>
      <th>Passive data point</th>
      <th>Start date time</th>
      <th>End date time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @effective_time_intervals.each do |effective_time_interval| %>
      <tr>
        <td><%= effective_time_interval.passive_data_point_id %></td>
        <td><%= effective_time_interval.start_date_time %></td>
        <td><%= effective_time_interval.end_date_time %></td>
        <td><%= link_to 'Show', effective_time_interval %></td>
        <td><%= link_to 'Edit', edit_effective_time_interval_path(effective_time_interval) %></td>
        <td><%= link_to 'Destroy', effective_time_interval, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Effective time interval', new_effective_time_interval_path %>
