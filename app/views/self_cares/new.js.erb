<% if params[:modal_enabled] == 'true' %>
  $("#inner-self_care-modal").html("<%= escape_javascript(render 'new') %>");  
  $("#self_care-modal").modal("show");
  $("#self_care-modal").css("z-index", "1500");
<% else %>
  $(".capture-window").html("<%= escape_javascript(render 'capture/new/self_care', :controller => 'self_cares', :action => 'new', :@self_care => SelfCare.new ) %>");
<% end %>