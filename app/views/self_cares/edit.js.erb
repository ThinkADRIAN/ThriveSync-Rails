<% if params[:modal_enabled] == 'true' %>
$("#inner-self_care-modal").html("<%= escape_javascript(render 'edit') %>");
$("#self_care-modal").appendTo("body").modal("show");
$("#self_care-modal").css("z-index", "1500");
<% else %>
$(".capture-delorean").html("<%= escape_javascript(render 'capture/delorean_disabled') %>");
$(".capture-nav").html("<%= escape_javascript(render 'capture/nav/self_care_disabled') %>");
$(".capture-window").html("<%= escape_javascript(render 'capture/new/self_care', :controller => 'self_cares', :action => 'edit', :@self_care => SelfCare.find(params[:id]) ) %>");
<% end %>